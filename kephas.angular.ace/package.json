    "@angular/forms": "^11.0.2",
    "@angular/platform-browser": "^11.0.2",
    "@kephas/angular": "^0.11.3",
    "@kephas/commands": "^0.4.3",
    "@kephas/core": "^0.4.3",
    "@kephas/messaging": "^0.4.3",
    "@kephas/reflection": "^0.4.5",
    "@kephas/ui": "^0.4.3",
    "@types/chai": "^4.2.11",
    "@types/jsdom": "^16.2.3",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.4",
