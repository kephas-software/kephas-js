{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@kephas/core": {
      "projectType": "library",
      "root": "projects/kephas/core",
      "sourceRoot": "projects/kephas/core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/kephas/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/kephas/core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/kephas/core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kephas/core/src/test.ts",
            "tsConfig": "projects/kephas/core/tsconfig.spec.json",
            "karmaConfig": "projects/kephas/core/karma.conf.js"
          }
        }
      }
    },
    "@kephas/reflection": {
      "projectType": "library",
      "root": "projects/kephas/reflection",
      "sourceRoot": "projects/kephas/reflection/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/kephas/reflection/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/kephas/reflection/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/kephas/reflection/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kephas/reflection/src/test.ts",
            "tsConfig": "projects/kephas/reflection/tsconfig.spec.json",
            "karmaConfig": "projects/kephas/reflection/karma.conf.js"
          }
        }
      }
    },
    "@kephas/messaging": {
      "projectType": "library",
      "root": "projects/kephas/messaging",
      "sourceRoot": "projects/kephas/messaging/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/kephas/messaging/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/kephas/messaging/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/kephas/messaging/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kephas/messaging/src/test.ts",
            "tsConfig": "projects/kephas/messaging/tsconfig.spec.json",
            "karmaConfig": "projects/kephas/messaging/karma.conf.js"
          }
        }
      }
    },
    "@kephas/commands": {
      "projectType": "library",
      "root": "projects/kephas/commands",
      "sourceRoot": "projects/kephas/commands/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/kephas/commands/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/kephas/commands/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/kephas/commands/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kephas/commands/src/test.ts",
            "tsConfig": "projects/kephas/commands/tsconfig.spec.json",
            "karmaConfig": "projects/kephas/commands/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "@kephas/core"
}
