    "@angular/common": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/platform-browser": "^11.0.2",
    "@kephas/angular": "^0.11.3",
    "@kephas/core": "^0.4.3",
    "@kephas/ui": "^0.4.3",
    "brace": "^0.11.1",
    "rxjs": "^6.5.3",
    "zone.js": "^0.10.3"
  },
