{
  "name": "@kephas/angular-ace",
  "version": "0.11.3",
  "description": "Provides the integration of the Ace editor with Kephas and Angular.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack & mocha",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "typedoc": "typedoc",
    "postinstall": "npm dedupe"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "reify"
    ],
    "spec": [
      "src/**/*.spec.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kephas-software/kephas-js/tree/master/kephas.angular.ace"
  },
  "keywords": [
    "kephas",
    "angular",
    "ace"
  ],
  "author": {
    "name": "Kephas Software SRL"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "dist/"
  ],
