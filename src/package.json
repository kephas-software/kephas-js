{
  "name": "kephas-js",
  "version": "1.0.0-preview.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "pack-core": "ng build --project=@kephas/core",
    "pack-reflection": "ng build --project=@kephas/reflection",
    "pack-messaging": "ng build --project=@kephas/messaging",
    "pack-commands": "ng build --project=@kephas/commands",
    "pack-ui": "ng build --project=@kephas/ui",
    "pack-ngx-core": "ng build --project=@kephas/ngx-core",
    "pack-ngx-ace": "ng build --project=@kephas/ngx-ace",
    "pack-ngx-oidc": "ng build --project=@kephas/ngx-oidc",
    "pack-all-fwk": "ng build --project=@kephas/core && ng build --project=@kephas/reflection && ng build --project=@kephas/messaging && ng build --project=@kephas/commands && ng build --project=@kephas/ui",
    "pack-all-ngx": "ng build --project=@kephas/ngx-core && ng build --project=@kephas/ngx-ace && ng build --project=@kephas/ngx-oidc",
    "publish-core": "cd ./dist/kephas/core && npm publish",
    "publish-reflection": "cd ./dist/kephas/reflection && npm publish",
    "publish-messaging": "cd ./dist/kephas/messaging && npm publish",
    "publish-commands": "cd ./dist/kephas/commands && npm publish",
    "publish-ui": "cd ./dist/kephas/ui && npm publish",
    "publish-all-fwk": "cd ./dist/kephas/core && npm publish && cd ../reflection && npm publish && cd ../messaging && npm publish && cd ../commands && npm publish && cd ../ui && npm publish",
    "publish-ngx-core": "cd ./dist/kephas/ngx-core && npm publish",
    "publish-ngx-ace": "cd ./dist/kephas/ngx-ace && npm publish",
    "publish-ngx-oidc": "cd ./dist/kephas/ngx-oidc && npm publish",
    "publish-all-ngx": "cd ./dist/kephas/ngx-core && npm publish && cd ../ngx-ace && npm publish && cd ../ngx-oidc && npm publish"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/animations": "~12.2.3",
    "@angular/common": "~12.2.3",
    "@angular/compiler": "~12.2.3",
    "@angular/core": "~12.2.3",
    "@angular/forms": "~12.2.3",
    "@angular/platform-browser": "~12.2.3",
    "@angular/platform-browser-dynamic": "~12.2.3",
    "@angular/router": "~12.2.3",
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.3",
    "@types/jasmine": "~3.8.2",
    "@types/node": "^16.7.8",
    "brace": "^0.11.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "js-sha256": "^0.9.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.2.1",
    "oidc-client": "^1.11.5",
    "rxjs": "~6.6.0",
    "ts-node": "~10.2.1",
    "tslint": "~6.1.3",
    "typescript": "~4.2.4",
    "zone.js": "~0.11.4"
  }
}
