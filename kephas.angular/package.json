{
  "name": "@kephas/angular",
  "version": "0.3.3",
  "description": "Provides integration capabilities with Angular.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack & mocha",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "typedoc": "typedoc"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "reify"
    ],
    "spec": [
      "src/**/*.spec.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kephas-software/kephas-js/tree/master/kephas.angular"
  },
  "keywords": [
    "kephas",
    "angular",
    "ng"
  ],
  "author": {
    "name": "Kephas Software SRL"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "dist/"
  ],
  "dependencies": {
    "@angular/common": "^8.2.7",
    "@angular/core": "^8.2.7",
    "@angular/forms": "^8.2.7",
    "@angular/platform-browser": "^8.2.7",
    "@kephas/commands": "^0.3.1",
    "@kephas/core": "^0.3.1",
    "@kephas/messaging": "^0.3.3",
    "@kephas/reflection": "^0.3.1",
    "@kephas/ui": "^0.3.1",
    "jwt-decode": "2.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/compiler": "^8.2.7",
    "@types/chai": "^4.2.11",
    "@types/jsdom": "^16.2.3",
    "@types/jwt-decode": "^2.2.1",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.4",
    "@types/webpack": "^4.41.13",
    "chai": "^4.2.0",
    "codelyzer": "6.0.0",
    "cross-env": "7.0.2",
    "jsdom": "^16.2.2",
    "mocha": "^7.1.2",
    "path": "^0.12.7",
    "reify": "^0.20.12",
    "rxjs-tslint": "0.1.8",
    "sinon": "^9.0.2",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "typescript": "3.5.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dts-bundle": "^0.0.2"
  }
}
